{
	"expo": {
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#ffffff",
				"foregroundImage": "./assets/adaptive-icon.png"
			},
			"userInterfaceStyle": "automatic"
		},
		"assetBundlePatterns": ["**/*"],
		"icon": "./assets/icon.png",
		"ios": {
			"supportsTablet": true,
			"userInterfaceStyle": "automatic"
		},
		"name": "scout-app",
		"description": "Scout app for the scout team. Developed by Aggutierrez. Used to manage all scout-related information incluiding CRUD operations, complex filters for searching and more.",
		"orientation": "portrait",
		"plugins": ["expo-router"],
		"scheme": "Scout app",
		"slug": "scout-app",
		"sourceExts": ["js", "json", "ts", "tsx", "jsx"],
		"splash": {
			"backgroundColor": "#ffffff",
			"image": "./assets/splash.png",
			"resizeMode": "contain"
		},
		"userInterfaceStyle": "automatic",
		"version": "1.0.0"
	}
}

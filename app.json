{
  "expo": {
    "name": "scout-app",
    "description": "Scout app for the scout team. Developed by Aggutierrez. Used to manage all scout-related information incluiding CRUD operations, complex filters for searching and more.",
    "orientation": "portrait",
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "scheme": "scoutapp",
    "slug": "scout-app",
    "splash": {
      "backgroundColor": "#0F0F0F",
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "userInterfaceStyle": "automatic",
    "version": "1.0.0",
    "icon": "./assets/scout-logo.png",
    "assetBundlePatterns": ["**/*"],
    "android": {
      "userInterfaceStyle": "automatic",
      "package": "com.aggutierrez.scoutapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptative-icon.png",
        "backgroundColor": "#0F0F0F"
      }
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.aggutierrez.scoutapp"
    },

    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "566a82f1-3bb4-44fa-8b49-d8cd70add0f3"
      }
    }
  }
}
